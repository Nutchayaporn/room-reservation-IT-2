# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Room {
  id: ID!
  name: String!
  status: String!
  capacity: Int!
  type: String!
}

type User {
  id: ID!
  name: String!
}

type Query {
  rooms: [Room!]!
  user: [User!]!

}

input NewRoom {
  name: String!
  capacity:Int!
  type:String!

}

input NewUser {
  name: String!
}

type Mutation {
  createRoom(input: NewRoom!): Room!
  createUser(input: NewUser!): User!
}
